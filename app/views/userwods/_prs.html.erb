
<h1>  These are your current PRs</h1>
	<ul>
		<% wods = current_user.userwods.by_pr.sort {|a,b| a.name <=> b.name} %>
		<% wods.each do |wod| %>
			<li><%= wod.name %></li>
			<li><%= wod.date.strftime("%d %B %Y") %></li>
			<li><%= wod.result %></li>
			___________________________________________
		<% end %>	
	</ul>	
