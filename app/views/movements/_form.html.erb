
<%= form_for @movement do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :date %>
	<%= date_select :movement, :date %>
	<%= f.label :result %>
	<%= f.text_field :result %>
	<%= f.label :movement_type %>
	<%= f.select :movement_type, ["time", "weight", "repetitions"], :prompt => "Select One" %>
	<%= f.label :pr %>
	<%= f.check_box :pr %>
<%= f.submit %>	
