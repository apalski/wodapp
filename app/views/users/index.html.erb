
<h1>Current Users:</h1>

<ul>
	<% @users.each do |user| %>
		<li>Name: <%= user.name %></li>
		<li>Email: <%= user.email %></li>
		<% if user != current_user  %>
			<li><%= link_to "Delete User", users_path(@user), method: :delete %></li>
		<% end %>	
		___________________________
	<% end %>
</ul>	
